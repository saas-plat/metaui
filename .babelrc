{
  "presets": [
    [
      "@babel/env", {
        "targets": {
          "browsers": ["last 2 Chrome versions", "IE >= 9"]
        }
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    ["@babel/proposal-decorators", {
      "legacy": true 
    }],
    ["@babel/proposal-class-properties", {
      "loose": true
    }],
    "@babel/proposal-export-default-from"
  ]
}
