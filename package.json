    "react-router-cache-route": ">=1.8.3",
    "saas-plat-expression": ">=1.0.0"
  },
  "devDependencies": {
    "mobx": "^5.15.0",
    "mobx-react": "^6.1.4",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-router-cache-route": "^1.8.3",
    "react-router-dom": "^5.1.2",
    "saas-plat-expression": "^1.0.1",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^4.13.1",
    "eslint-plugin-react": "^7.17.0",
    "mocha": "^6.1.4",
    "saas-plat-clientTools": "^1.0.70"
