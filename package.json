  },
  "peerDependencies": {
    "mobx": ">=5.0.0",
    "mobx-react": ">=6.0.0",
    "react": ">=16.0.0",
    "prop-types": ">=15.6.0",
    "react-dom": ">=16.0.0",
    "react-router": ">=5.0.0",
