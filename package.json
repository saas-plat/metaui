  "main": "dist/index.js",
  "scripts": {
    "i": "npm i --registry https://npm.saas-plat.com",
    "up": "npm update --registry https://npm.saas-plat.com",
    "build": "node node_modules/saas-plat-clientTools/bin/build",
    "pub": "node node_modules/saas-plat-clientTools/bin/publish",
    "lpub": "npm run build && npm publish --registry https://npm.saas-plat.com",
    "dev": "cross-env DISTPATH=../saas-plat-metauiweb/node_modules/saas-plat-metaui/dist npm run build",
    "dev_m": "cross-env DISTPATH=../saas-plat-metauimobile/node_modules/saas-plat-metaui/dist npm run build",
    "dev_t": "cross-env DISTPATH=../saas-plat-metauitouch/node_modules/saas-plat-metaui/dist npm run build",
    "watch": "cross-env DEBUG=saas-plat:*,-not_this mocha --watch",
    "test": "cross-env DEBUG=saas-plat:*,-not_this mocha"
  },
  "repository": {
    "type": "git",
    "url": "git@saas-plat.com:saas-plat-metaui"
  },
  "keywords": [
    "metaui"
  ],
